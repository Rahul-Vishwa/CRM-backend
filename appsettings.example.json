{
    "ConnectionStrings": {
        "DefaultConnection": "Host=<HOST>;Port=<PORT>;Database=<DB_NAME>;Username=<USERNAME>;Password=<PASSWORD>"
    },
    "JwtSettings": {
        "Issuer": "<ISSUER>",
        "Audience": "<AUDIENCE>",
        "Key": "<YOUR_SECRET_KEY>"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}
